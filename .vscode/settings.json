{
  "redhat.telemetry.enabled": true,
  "files.associations": {
    "*.yml": "ansible"
  },
  "files.autoSave": "onFocusChange",
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,
  "editor.wordBasedSuggestions": "allDocuments",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "prettier.endOfLine": "auto",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "security.workspace.trust.banner": "always",
  "ansible.python.interpreterPath": "/usr/bin/python3",
  "ansible.executionEnvironment.enabled": false,
  "ansible.lightspeed.enabled": true,
  "ansible.lightspeed.suggestions.enabled": true,
  "ansibleServer.trace.server": "messages",
  "ansible.ansible.reuseTerminal": true,
  "ansibleLint.enabled": true,
  "ansibleLint.arguments": ["--exclude", "roles", "--exclude", "tests"],
  "ansible.useFullyQualifiedCollectionNames": true,
  "ansible.validation.lint": true,
  "ansible.validation.lintOnSave": true,
  "ansible.validation.run": "onType",
  "ansible.completion.resolveModules": true,
  "ansible.completion.resolveRoles": true,
  "ansible.hover.documentation": true,
  "ansible.hover.module": true,
  "ansible.hover.task": true,
  "ansible.hover.variable": true
}
